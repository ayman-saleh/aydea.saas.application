{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "clean": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": true
    },
    "billing:sync": {
      "cache": false
    },
    "db:migrate": {
      "dependsOn": ["db:generate"],
      "inputs": ["drizzle/**"]
    },
    "db:generate": {
      "inputs": ["src/**", "drizzle.config.ts"],
      "outputs": ["drizzle/**"]
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "db:check": { "cache": false },
    "db:push": { "cache": false }
  }
}
