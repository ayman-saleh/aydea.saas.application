    "@chakra-ui/react": "2.10.9",
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "@saas-ui/react": ">=2.0.0",
    "framer-motion": ">=6",
    "react": "18.3.1",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react-icons": "^5.3.0",
    "tsup": "^8.5.0"
