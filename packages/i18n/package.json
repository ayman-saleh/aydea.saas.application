{
  "name": "@acme/i18n",
  "main": "src/index.ts",
  "version": "0.5.3",
  "private": true,
  "scripts": {
    "extract": "formatjs extract './src/**/*.ts*' '../ui/**/*.ts*' '../../apps/web/{app,features}/**/*.ts*' --flatten",
